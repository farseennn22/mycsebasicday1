#include<stdio.h>
int main() {
    int mark;
    printf("Enter the students marks:");
    scanf("%d",&mark);
    if(mark >=40) {
        printf("the student has passed.\n");
    } else {
        printf("The student has failed.\n");
    }
    return 0;    
    }
